{
	"name": "docker-ts",
	"description": "",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"keywords": [
		"bot",
		"discord",
		"discord.js",
		"robo",
		"robo.js",
		"typescript"
	],
	"scripts": {
		"build": "robo build",
		"deploy": "robo deploy",
		"dev": "robox dev",
		"doctor": "sage doctor",
		"docker:build": "docker buildx build --progress=plain --load --platform linux/arm64/v8 -t docker-example .",
		"docker:start": "docker run --name docker-example docker-example",
		"docker:stop": "docker stop docker-example && docker rm docker-example",
		"invite": "robo invite",
		"lint": "prettier --write .",
		"start": "robo start",
		"upgrade": "sage upgrade"
	},
	"dependencies": {
		"discord.js": "^14.19.3",
		"robo.js": "^0.10.30"
	},
	"devDependencies": {
		"@swc/core": "^1.11.29",
		"@types/node": "^22.15.27",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3"
	}
}
