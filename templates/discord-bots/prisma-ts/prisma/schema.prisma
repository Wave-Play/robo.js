generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model users {
    id    Int     @id @default(autoincrement())
    name  String?
    email String?
    age   Int?
}
cd .\starter-js\; pnpm update;  cd .\starter-ts\; pnpm update;  cd .\tagbot\; pnpm update
