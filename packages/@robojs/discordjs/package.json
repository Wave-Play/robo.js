{
	"name": "@robojs/discordjs",
	"description": "Discord.js integration plugin for Robo.js - provides commands, events, and context menus",
	"version": "0.0.0",
	"type": "module",
	"private": false,
	"keywords": [
		"bot",
		"discord",
		"discord.js",
		"robo",
		"robo.js",
		"typescript",
		"plugin"
	],
	"main": ".robo/build/index.js",
	"types": ".robo/build/index.d.ts",
	"exports": {
		".": {
			"import": "./.robo/build/index.js",
			"types": "./.robo/build/index.d.ts"
		},
		"./types": {
			"import": "./.robo/build/types/index.js",
			"types": "./.robo/build/types/index.d.ts"
		}
	},
	"license": "MIT",
	"author": "Robo.js",
	"contributors": [
		"Robo.js Team"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Wave-Play/robo.js.git",
		"directory": "packages/@robojs/discordjs"
	},
	"files": [
		".robo/",
		"src/",
		"LICENSE",
		"README.md"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"scripts": {
		"build": "robo build plugin",
		"dev": "robo build plugin --watch",
		"lint:style": "prettier --write .",
		"prepublishOnly": "robo build plugin"
	},
	"devDependencies": {
		"@swc/core": "^1.15.3",
		"@types/node": "^24.10.1",
		"discord.js": "14.25.1",
		"prettier": "^3.7.3",
		"robo.js": "workspace:*",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"discord.js": "^14.0.0",
		"robo.js": "^0.11.0"
	},
	"peerDependenciesMeta": {
		"robo.js": {
			"optional": false
		},
		"discord.js": {
			"optional": false
		}
	}
}